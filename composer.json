{
    "name": "risetechapps/repository-for-laravel",
    "description": "",
    "keywords": [
        "risetechapps",
        "Rise Tech",
        "repository",
        "Model"
    ],
    "homepage": "https://github.com/risetechapps/repository-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12",
        "risetechapps/has-uuid-for-laravel": "^1.0.0",
        "risetechapps/to-upper-for-laravel": "^1.0.0",
        "tpetry/laravel-postgresql-enhanced": "^3.5.0",
        "predis/predis": "^v2.2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTechApps\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTechApps\\Repository\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTechApps\\Repository\\RepositoryServiceProvider"
            ],
            "aliases": {
                "Repository": "RiseTechApps\\Repository\\RepositoryFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.4.0"
}
